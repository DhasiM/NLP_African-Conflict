ctors)]\n\n    return (words, word_values[0,:n_top_words].tolist()[0])","metadata":{"execution":{"iopub.status.busy":"2022-04-20T09:48:01.085693Z","iopub.execute_input":"2022-04-20T09:48:01.085953Z","iopub.status.idle":"2022-04-20T09:48:01.093457Z","shell.execute_reply.started":"2022-04-20T09:48:01.085924Z","shell.execute_reply":"2022-04-20T09:48:01.092460Z"},"trusted":true},"execution_count":190,"outputs":[]},{"cell_type":"code","source":"count_vectorizer = CountVectorizer(stop_words='english')\nwords, word_values = get_top_n_words(n_top_words=15,\n                                     count_vectorizer=count_vectorizer, \n                                     text_data=reindexed_data)\n\nfig, ax = plt.subplots(figsize=(16,8))\nax.bar(range(len(words)), word_values);\nax.set_xticks(range(len(words)));\nax.set_xticklabels(words, rotation='vertical');\nax.set_title('Top words in african conflict dataset (excluding stop words)');\nax.set_xlabel('Word');\nax.set_ylabel('Number of occurences');\nplt.show()","metadata":{"execution":{"iopub.status.busy":"2022-04-20T09:48:02.821215Z","iopub.execute_input":"2022-04-20T09:48:02.821759Z","iopub.status.idle":"2022-04-20T09:48:09.599810Z","shell.execute_reply.started":"2022-04-20T09:48:02.821719Z","shell.execute_reply":"2022-04-20T09:48:09.599062Z"},"trusted":true},"execution_count":191,"outputs":[{"output_type":"display_data","data":{"text/plain":"<Figure size 1152x576 with 1 Axes>","image/png":"\n"},"metadata":{"needs_background":"light"}}]},{"cell_type":"code","source":"tagged_headlines = [TextBlob(reindexed_data[i]).pos_tags for i in range(reindexed_data.shape[0])]","metadata":{"execution":{"iopub.status.busy":"2022-04-20T09:48:11.462424Z","iopub.execute_input":"2022-04-20T09:48:11.462680Z","iopub.status.idle":"2022-04-20T10:03:05.377954Z","shell.execute_reply.started":"2022-04-20T09:48:11.462651Z","shell.execute_reply":"2022-04-20T10:03:05.376091Z"},"trusted":true},"execution_count":192,"outputs":[{"traceback":["\u001b[0;31m-